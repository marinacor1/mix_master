<%= stylesheet_link_tag "application.css" %>
<h1>All Artists</h1>
<p><%= link_to "New artist", new_artist_path %></p>
<table border = "1">
  <% @artists.map do |artist| %>
  <tr>
  <td><%= link_to artist.name, artist_path(artist)%></td>
<td><%= image_tag artist.image_path %></td>
<td><%= image_tag artist.discography %></td>
</tr>
<% end  %>
</table>
<p><%= link_to "All Songs", songs_path %></p>
<p><%= link_to "All Playlists", playlists_path %></p>
